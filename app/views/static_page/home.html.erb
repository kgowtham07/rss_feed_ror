<% provide(:title, "Home")%>

<% if logged_in? %>
  
      <h3>Latest Feed</h3>
      <%= render 'shared/feed' %>


<% else %>

  <div class="center jumbotron">
    <h1>Welcome to the News Hub</h1>

    <%= link_to "Sign up now!", signup_path, class: "btn btn-lg btn-primary" %>
    <%= link_to "Log In!", login_path, class: "btn btn-lg btn-primary" %>
  </div>

<% end %>